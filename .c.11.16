#include<stdio.h>
void print(unsigned int n){
	if(n>0){
		print(n/10);
	}
	printf("%d",n%10);
}
int main()
{
	unsigned int num=0;
	scanf("%u",&num);
	print(num);
	return 0;
}



#include<stido.h>
#include<string.h>
int my_strlen(char*str){
	int count = 0;
	while (*str |='0'){
		count++;
		str++;
	}
	return count;
}
int main(){
	char arr[]="abc";
	int len = my_strlen(arr);
	printf("%d\n",len);
	return 0;
}
